$this->modelBooks = new Publisher_Model_DbTable_Books();
$genresearchUrl=$this->serverUrl().$this->baseUrl().'/search/index/publication/genre/id/'; 
for($x=0;$x<count($this->getmagazine_genres);$x++)
{
	 
	 
	 $genrecount = $this->modelBooks->getGenreCount($this->getmagazine_genres[$x]['product_type']);
 
	 
?>
<li><a href="<?php echo $genresearchUrl.$this->getmagazine_genres[$x]['product_type'];?>"><?php echo $this->getmagazine_genres[$x]['genre']."(". count($genrecount).")";?></a></li>
<?php } ?>
<div class="last_more"><a href="javascript:void();" class="more_magazine">More</a></div>
<div style="display:none;" id="hide_magazine">
<li><a href="#">sasac</a></li>
<li><a href="#">sacsa</a></li>
