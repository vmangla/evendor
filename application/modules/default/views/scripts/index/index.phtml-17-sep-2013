if(isset($this->approvedAndFeaturedBookList) && !empty($this->approvedAndFeaturedBookList))
{
?>
<h1>Featured eBooks <a href="<?php echo $this->serverUrl().$this->baseUrl();?>/catalogue/index/publication/ebooks/">/ &nbsp;&nbsp;View all</a></h1>
<?php
	$modelAuthor = new Publisher_Model_DbTable_Publishers();
	$modelImage = new Publisher_Model_DbTable_BookImages();
?>
	<!--<h1>Featured eBooks</h1>-->
		<?php if(count($this->approvedAndFeaturedBookList)>4)
		{
