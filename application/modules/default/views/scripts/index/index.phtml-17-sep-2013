				<?php
				foreach($this->approvedAndFeaturedBookList as $key=>$ebookArray)
				{
					$authorInfo = $modelAuthor->getInfoByPublisherId($ebookArray['author_id']);
					$imageInfo = $modelImage->getImageInfoByProductId($ebookArray['product_id']);
					
				    $productPriceInfo = $productPrice->getPriceByStoreId($ebookArray['product_id'],$ebookArray['country_id']);
					?>
					<li class="jcarousel-item jcarousel-item-horizontal jcarousel-item-<?php echo $key+1;?> jcarousel-item-<?php echo $key+1;?>-horizontal">
